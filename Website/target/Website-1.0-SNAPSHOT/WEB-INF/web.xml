<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>Data</servlet-name>
        <servlet-class>com.mycompany.website.Data</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DisplayLogin</servlet-name>
        <servlet-class>com.mycompany.website.DisplayLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FetchEvents</servlet-name>
        <servlet-class>com.mycompany.website.FetchEvents</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FetchEvents1</servlet-name>
        <servlet-class>com.mycompany.website.FetchEvents1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>event_ticket</servlet-name>
        <servlet-class>com.mycompany.website.event_ticket</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>My_Events</servlet-name>
        <servlet-class>com.mycompany.website.My_Events</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FeedbackServlet</servlet-name>
        <servlet-class>com.mycompany.website.FeedbackServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ContactUsServlet</servlet-name>
        <servlet-class>com.mycompany.website.ContactUsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>mainServlet</servlet-name>
        <servlet-class>com.mycompany.website.mainServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Data</servlet-name>
        <url-pattern>/Data</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DisplayLogin</servlet-name>
        <url-pattern>/DisplayLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FetchEvents</servlet-name>
        <url-pattern>/FetchEvents</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FetchEvents1</servlet-name>
        <url-pattern>/FetchEvents1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>event_ticket</servlet-name>
        <url-pattern>/event_ticket</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>My_Events</servlet-name>
        <url-pattern>/My_Events</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FeedbackServlet</servlet-name>
        <url-pattern>/FeedbackServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ContactUsServlet</servlet-name>
        <url-pattern>/ContactUsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>mainServlet</servlet-name>
        <url-pattern>/mainServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
